<style>
  #chartdiv { width: 100%; height: 150px;}
</style>

<!-- Chart code -->
<script>
  am4core.ready(function() { // Themes begin
  am4core.useTheme(am4themes_material); am4core.useTheme(am4themes_animated);
  var chart = am4core.create("chartdiv", am4charts.PieChart); chart.data = [ {
  "category": "Food", "dollars": 500 }, { "category": "Entertainment",
  "dollars": 200 },{ "category": "Transportation", "dollars": 280 }, {
  "category": "Shopping", "dollars": 100 }, { "category": "Payments", "dollars":
  210 }, { "category": "Travel", "dollars": 175 },]; var pieSeries =
  chart.series.push(new am4charts.PieSeries()); pieSeries.dataFields.value =
  "dollars"; pieSeries.labels.template.fill = am4core.color("black");
  pieSeries.labels.template.disabled = false; pieSeries.ticks.template.disabled
  = false; pieSeries.dataFields.category = "category";
  pieSeries.slices.template.stroke = am4core.color("white");
  pieSeries.slices.template.strokeWidth = 2;
  pieSeries.slices.template.strokeOpacity = 1;
  pieSeries.hiddenState.properties.opacity = 1;
  pieSeries.hiddenState.properties.endAngle = -90;
  pieSeries.hiddenState.properties.startAngle = -90; }); am4core.ready()
</script>